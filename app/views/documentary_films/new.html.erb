<h2> Ingresa tu documental </h2>

<%= form_with model: @documentary, method: :post do |form| %>
 <div>
 <%= form.label :name, " Documentary Title" %>
 <%= form.text_field :name %>
 </div>
 <div>
 <%= form.label :synopsis, "Documentary Synopsis" %>
 <%= form.text_field :synopsis %>
 </div>
<div>
 <%= form.label :director, "Documentary Director" %>
 <%= form.text_field :director %>
 </div>
 <div>
 <%= form.submit %>
 </div>
<% end %>